generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lyric {
  id        Int     @id @default(autoincrement())
  text      String
  is_reaction Boolean
  paragraph_count Int
  word_count Int
  hashtag_count Int
  hashtags String[]
}