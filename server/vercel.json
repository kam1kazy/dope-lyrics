{
    "cleanUrls": true,
    "installCommand": "bun install",
    "buildCommand": "bun run build:vercel",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "server/src/index.ts"
        }
    ],
    "functions": {
        "server/src/index.ts": {
            "maxDuration": 60
        }
    }
}